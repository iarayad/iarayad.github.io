<table class="table talks-table">
  <thead>
    <tr>
      <th>Year</th>
      <th>Event</th>
      <th>Talk</th>
      <th>Location</th>
      <th>Role</th>
    </tr>
  </thead>
  <tbody>
    <% for (const item of items) { %>
    <tr>
      <td><%= new Date(item.date).getFullYear() %></td>
      <td><%= item.event %></td>
      <td><%= item.title %></td>
      <td><%= item.location %></td>
      <td><%= item.role %></td>
    </tr>
    <% } %>
  </tbody>
</table>
